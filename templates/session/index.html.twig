               <th>Nom Session</th>
               <th>Date d√©but</th>
               <th>Date fin</th>
               <th>Nb de places disponibles</th>
            </tr>
        </thead>
            {% for session in sessions %}
               <tr> 
                  <td>{{ session.nom }}</td>
                  <td>{{ session.dateD | date('d/m/Y') }}</td>
